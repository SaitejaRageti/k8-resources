apiVersion: v1
kind: Service
metadata:
  name: nginx-service
  labels:
    purpose: service-demo
spec:
  selector:
    project: roboshop
    environment: dev
    purpose: service-demo
    
  ports:
    - protocol: TCP
      port: 80 ##serviceport
      targetPort: 80 ##targetport
---
apiVersion: v1
kind: Pod
metadata: 
  name: nginx
  labels:
    project: roboshop
    environment: dev
    purpose: service-demo
spec:
  containers:
    - name: nginx
      image: nginx


  